issn = {10106030},
journal = {Journal of Photochemistry and Photobiology A: Chemistry},
keywords = {-thiophene and -pyridine derivatives,Fluorescence/photoisomerization competition,Push-pull distyryl-furan,Synthesis of distyrylbenzene analogues},
month = {sep},
pages = {38--46},
title = {{Synthesis, spectral properties and photobehaviour of push–pull distyrylbenzene nitro-derivatives}},
