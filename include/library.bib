    issn = {01437208},
    journal = {Dyes and Pigments},
    keywords = {Chromophore,Donor,Electro-optic,Nonlinear optical materials,Second-order,Thermal stability},
    month = {jul},
    pages = {138--147},
    publisher = {Elsevier Ltd},
    title = {{The design of nonlinear optical chromophores exhibiting large electro-optic activity and high thermal stability: The role of donor groups}},
