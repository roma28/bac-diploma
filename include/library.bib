    journal = {Journal of Biotechnology},
    keywords = {Electrodialysis,Energy/current efficiency,Lactate,Purification},
    number = {3},
    pages = {223--239},
    pmid = {12044551},
    title = {{Process development and optimisation of lactic acid purification using electrodialysis}},
