issn = {09608524},
journal = {Bioresource Technology},
keywords = {Fed-batch,Kinetics,Lactobacillus casei,Probiotic,Whey},
number = {8},
pages = {2837--2844},
publisher = {Elsevier Ltd},
title = {{Production of probiotic biomass (Lactobacillus casei) in goat milk whey: Comparison of batch, continuous and fed-batch cultures}},
url = {http://dx.doi.org/10.1016/j.biortech.2009.10.047},
volume = {101},
