journal = {Journal of Chemical Technology {\&} Biotechnology},
keywords = {Peracetico. Ca{\~{n}}a de azucar},
number = {May},
pages = {1163--1169},
pmid = {17034937},
title = {{Lactic acid: recent advances in products, processes and technologies – a review}},
volume = {81},
year = {2006}
}
