isbn = {0306-2619},
issn = {03062619},
journal = {Applied Energy},
keywords = {CO2emissions,Coal power plant,District heating,Eco-efficiency,Global warming,Waste heat recovery},
month = {mar},
pages = {789--799},
title = {{Application of eco-efficiency in a coal-burning power plant benefitting both the environment and citizens: Design of a ‘city water heating' system}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0306261915012246},
